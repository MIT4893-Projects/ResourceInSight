<Application x:Class="ResourceInSight.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ResourceInSight"
             StartupUri="Views/OnScreenOverlayView.xaml">
    <Application.Resources>
        <FontFamily x:Key="FontAwesomeSolid">pack://application:,,,/Fonts/#Font Awesome 6 Free Solid</FontFamily>
        <FontFamily x:Key="FontAwesomeRegular">pack://application:,,,/Fonts/#Font Awesome 6 Brands Regular</FontFamily>
        <FontFamily x:Key="GoogleSans">pack://application:,,,/Fonts/#Google Sans</FontFamily>
    </Application.Resources>
</Application>
